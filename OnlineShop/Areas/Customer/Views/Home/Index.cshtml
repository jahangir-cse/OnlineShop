@using OnlineShop.Models;
@using X.PagedList.Mvc.Core
@model X.PagedList.IPagedList<Products>
@{
    ViewData["Title"] = "Home Page";
}
<br />
<!--Slide part-->
<section>
    <div class="carousel_section bg-dark">
        <div id="myCarousel" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
                <li data-slide-to="0" data-target="#myCarousel" class="active"></li>
                <li data-slide-to="1" data-target="#myCarousel"></li>
                <li data-slide-to="2" data-target="#myCarousel"></li>
                <li data-slide-to="3" data-target="#myCarousel"></li>
            </ol>
            <div class="carousel-inner">
                <div class="carousel-item carousel-img-1 active">
                    <div class="container">
                        <div class="carousel-caption text-left">
                            <div class="row">
                                <div class="col-md-7 pt-5">
                                    <h1 class="display-3 w3-animate-left pt-2">Brand New</h1>
                                    <h4 class="w3-animate-top pt-2">Transforming Bangladesh into digital</h4>
                                    <a href="#" class="btn btn-danger w3-animate-right mt-2">Click For Details</a>
                                </div>
                                <div class="col-md-5">
                                    <img src="~/images/image2.png" class="img-fluid" alt="image" />
                                </div>
                            </div>                           
                        </div>
                    </div>
                </div>
                <div class="carousel-item carousel-img-2">
                    <div class="container">
                        <div class="carousel-caption text-left">
