@using OnlineShop.Models;
@using X.PagedList.Mvc.Core
@model X.PagedList.IPagedList<Products>
@{
    ViewData["Title"] = "Home Page";
}
<style>
    .viewAction {
        border-radius: 26px;
        background: linear-gradient(to right, #c79d03,#fb2f5e);
        padding: 7px 20px;
        text-decoration: none;
        text-align: center;
        color: #fff;
    }
</style>
<br />
<!--Slide part-->
<section id="showcase" class="bg-dark">
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-slide-to="0" data-target="#myCarousel" class="active"></li>
            <li data-slide-to="1" data-target="#myCarousel"></li>
            <li data-slide-to="2" data-target="#myCarousel"></li>
        </ol>
        <div class="carousel-inner">
            <div class="carousel-item carousel-img-1 active">
                <div class="container">
                    <div class="carousel-caption mb-5 pb-5">
                        <h2 class="display-4 w3-animate-left">New Fall Winter Collection</h2>
                        <p class="lead w3-animate-right">Shop The Latest Fashion</p>
                        <a href="#" class="btn btn-danger w3-animate-bottom">Learn More</a>
                    </div>
                </div>
            </div>
            <div class="carousel-item carousel-img-2">
                <div class="container">
                    <div class="carousel-caption mb-5 pb-5">                        
                        <h2 class="display-4 w3-animate-left">Let us think about your fashion</h2>
                        <p class="lead w3-animate-right">Shop The Latest Fashion</p>
                        <a href="#" class="btn btn-danger w3-animate-bottom">Learn More</a>
                    </div>
                </div>
            </div>
            <div class="carousel-item carousel-img-3">
                <div class="container">
                    <div class="carousel-caption mb-5 pb-5">
                        <h2 class="display-4 w3-animate-left">Fashion with the show costume designer</h2>
                        <p class="lead w3-animate-right">Shop The Latest Fashion</p>
                        <a href="#" class="btn btn-danger w3-animate-bottom">Learn More</a>
                    </div>
                </div>
            </div>
        </div>
        <a href="#myCarousel" class="carousel-control-prev" data-slide="prev">
            <span class="carousel-control-prev-icon"></span>
        </a>
        <a href="#myCarousel" class="carousel-control-next" data-slide="next">
            <span class="carousel-control-next-icon"></span>
        </a>
    </div>
</section>
<!--Main Part-->
<section>
    <div class="row">
        <div class="col-md-2 container">
            <div class="sidebar">
                <h3 class="cat-heading text-center py-1" style="font-size:x-large"><i class="fa fa-bars mr-1" aria-hidden="true"></i>CATEGORIES</h3>
                <hr />
                @*<a href="#home">Electronics</a>*@
                <div class="dropdown">
                    <button class="dropbtn">Electronics  <i class="fa fa-caret-down"></i></button>
                    <div class="dropdown-content">
                        <a href="#">Laptop</a>
                        <a href="#">Mobile</a>
                        <a href="#">Television</a>
                    </div>
                </div>
                <a href="#news">Home & Living</a>
                <a href="#contact">Books</a>
                <a href="#about">Stationeries</a>
                <a href="#contact">Furniture</a>
                <a href="#contact">Men's Fashion</a>
                <a href="#contact">Women's Fashion</a>
                <a href="#contact">Home Appliances</a>
                <a href="#contact">Health</a>
                <a href="#contact">Beauty</a>
                <a href="#contact">Fitness</a>
                <a href="#contact">Kides & Babies</a>
                <a href="#contact">Others</a>
            </div>
        </div>
        <div class="col-md-10 container">
            <h2 class="text-center text-primary">On Sale</h2>
            <hr />
            <div class="row">
                @foreach (var product in Model)
                {
                    <div class="col-md-2">
                        <div class="product-top">
                            <img src="~/@product.Image" style="width:100%;height:200px;" />
                            <div class="overlay-right">
                                <a asp-action="Details" asp-controller="Home" asp-route-id="@product.Id" class="btn btn-secondary pull-right btn-primary" title="View Details">
                                    <i class="fas fa-info-circle" style="font-size:30px;"></i>
                                </a>
                                <button type="button" class="btn btn-secondary" title="Quick Shop"><i class="far fa-eye" style="font-size:30px;"></i></button>
                                <button type="button" class="btn btn-secondary" title="Add to Wishlist"><i class="fas fa-heart" style="font-size:30px;"></i></button>
                                <a asp-action="Cart" asp-controller="Home" type="button" class="btn btn-secondary" title="Add to Cart"><i class="fas fa-cart-plus" style="font-size:30px;"></i></a>
                                @*<div class="container">
                                    @if (product != null)
                                    {
                                        <button type="submit" class="btn btn-danger" asp-area="Customer" asp-controller="Home" asp-action="Remove" asp-route-id="@product.Id"><i class="fas fa-cart-plus"></i></button>
                                    }
                                    else
                                    {
                                        <button type="submit" class="btn btn-danger" asp-area="Customer" asp-controller="Home" asp-action="Details" asp-route-id="@product.Id"><i class="fas fa-cart-plus"></i></button>
                                    }
                                </div>*@
                            </div>
                        </div>
                        <div class="product-bottom text-center">
                            <i class="fa fa-star" style="color:orange;"></i>
                            <i class="fa fa-star" style="color:orange;"></i>
                            <i class="fa fa-star" style="color:orange;"></i>
                            <i class="fa fa-star" style="color:orange;"></i>
                            <i class="fa fa-star-half" style="color:orange;"></i>
                            <h3>@product.Name</h3>
                            <h5>@product.Price</h5>
                        </div>
                    </div>
                }
            </div>
            <div class="align-content-center">
                @Html.PagedListPager(Model, c => Href("~/Customer/Home/Index?page=" + c))
            </div>
        </div>
    </div>
</section>
